{
  "name": "dionysus-bot",
  "version": "1.0.0",
  "description": "discord bot to manage game servers",
  "main": "index.js",
  "scripts": {
    "dev": "npx ts-node ./src/index.ts",
    "build": "tsc",
    "addExecPermissions": "chmod +x ./scripts/**/*.sh",
    "prod": "node ./dist/index.js",
    "pm2": "pm2 start ./dist/index.js --name dionysus-bot -- start",
    "initActivity": "mkdir ./activity; touch ./activity/minecraft ./activity/satisfactory ./activity/valheim ./activity/factorio"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoodEffort/dionysus-bot.git"
  },
  "author": "Luke Hovarter",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GoodEffort/dionysus-bot/issues"
  },
  "homepage": "https://github.com/GoodEffort/dionysus-bot#readme",
  "dependencies": {
    "@discordjs/builders": "^1.4.0",
    "@discordjs/rest": "^1.5.0",
    "discord-api-types": "^0.37.27",
    "discord.js": "^14.7.1"
  },
  "devDependencies": {
    "typescript": "^4.9.4"
  }
}
